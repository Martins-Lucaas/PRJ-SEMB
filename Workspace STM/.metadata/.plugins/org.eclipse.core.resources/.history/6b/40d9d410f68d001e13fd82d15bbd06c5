extern bt_sair;
extern bt_oxy;
extern bt_temp;
extern bt_emg;

void init(void)
{
	if (bt_eletro = 1){

		void emg_init(void);
	}
	if (bt_oxy = 1){

			void bt_oxy(void);
	}
	if (bt_temp = 1){

			void emg_init(void);
	}
	if (bt_emg = 1){

			void emg_init(void);
	}
}

