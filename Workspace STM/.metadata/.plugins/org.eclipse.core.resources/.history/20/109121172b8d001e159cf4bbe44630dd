#include <stdint.h>
#include "main.h"

extern TIM_HandleTypeDef hadc1;


void emg_system(void)
{
	//uint16_t emgplot = 0;
	//char buffer[20];
	HAL_ADC_Start(&hadc1);
	HAL_ADC_Stop(&hadc1);
